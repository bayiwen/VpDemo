<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable = "true"
    android:focusableInTouchMode="true">


    <TextView
        android:text="请输入美标线号（AWG）"
        android:textSize="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp" />

    <EditText
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:ems="8"
        android:hint="输入线号"
        android:id="@+id/et_AWG"
        android:inputType="number"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp" />

    <Button
        android:text="查询"
        android:textSize="24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/btn_query"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/et_AWG"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp" />

    <TextView
        android:text="查询结果"
        android:textSize="24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:id="@+id/textView2"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.39"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_AWG"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        android:layout_marginLeft="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_diameter"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        android:layout_marginLeft="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_area"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginLeft="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_resistance"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginLeft="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_current"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView9"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="      "
        android:textSize="20dp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_MAXcurrent"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10" />

    <TextView
        android:text="面积"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintLeft_toLeftOf="@+id/textView6" />
    <TextView
        android:text="直径"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintLeft_toLeftOf="@+id/textView8" />

    <TextView
        android:text="阻值"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintLeft_toLeftOf="@+id/textView5" />
    <TextView
        android:text="线号"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.48000002"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />
    <TextView
        android:text="通流量"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toLeftOf="@+id/textView7" />

    <TextView
        android:text="最大通流量"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toLeftOf="@+id/textView9"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />
    <TextView
        android:text="mm"
        android:textSize="20dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_diameter"
        android:layout_marginLeft="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_diameter" />

    <TextView
        android:text="@string/cable_area"
        android:textSize="20dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toRightOf="@+id/tv_area"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_area"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp" />
    <TextView
        android:text="A"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_MAXcurrent"
        android:layout_marginLeft="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_MAXcurrent" />

    <TextView
        android:text="A"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_current"
        android:layout_marginLeft="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_current" />

    <TextView
        android:text="Ω/Km"
        android:textSize="20dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_resistance"
        android:layout_marginLeft="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_resistance" />


</android.support.constraint.ConstraintLayout>